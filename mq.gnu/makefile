#############################################################################
#
# MQ MAKEFILE
#
#############################################################################


CC=gcc
FLAGS=-s

OBJS=mq.o

.c.o:
	$(CC) -c -O2 -Wall -m486 $< -o $@

all: mq.exe

mq.exe: $(OBJS)
	$(CC) $(FLAGS) $(OBJS) -o $@

mq.o: mq.c
