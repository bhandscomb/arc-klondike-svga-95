#############################################################################
#
# CMPR MAKEFILE
#
#############################################################################


CC=gcc
FLAGS=-s

OBJS=cmpr.o

.c.o:
	$(CC) -c -O2 -Wall -m486 $< -o $@

all: cmpr.exe

cmpr.exe: $(OBJS)
	$(CC) $(FLAGS) $(OBJS) -o $@

cmpr.o: cmpr.c
